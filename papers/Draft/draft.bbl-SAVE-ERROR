      \field{title}{Age, Sex, and Vowel Dependencies of Acoustic Measures Related to the Voice Sourcea)}
      \field{volume}{121}
      \field{year}{2007}
      \field{dateera}{ce}
      \field{pages}{2283\bibrangedash 2295}
