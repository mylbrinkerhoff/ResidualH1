    \entry{blankenshipTimeCourseBreathiness1997}{thesis}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=1a34a3de891ef3377a4ce247b638c5b1}{%
           family={Blankenship},
           familyi={B\bibinitperiod},
